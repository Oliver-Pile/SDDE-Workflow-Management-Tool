
<header class="project-header"> 
  <h1>Projects</h1>
  <div class="project-links">
    <%= link_to "Add Project", new_project_path(), class: "btn btn-primary m-2"%>
  </div>
</header>

<div id="projects">
    <div class="row">
      <% @projects.active.each do |project| %>
        <%= render project %>
      <% end %>
    </div>
</div>
